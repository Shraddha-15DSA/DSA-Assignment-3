#include<iostream>
using namespace std;
int main(){
    int n=6;
    int arr[n]={1,1,2,2,2,3};
    int X=2;
    int count=0;
    for(int i=0;i<n;i++){
        if(arr[i]==X){
            count=count+1;
        }

        
    }
    cout<<"Occurence of element X is "<<count<<endl;
    
    return 0;
}
